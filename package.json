{
  "name": "trashable-react",
  "version": "1.0.7",
  "description": "A HOC to make React Components garbage collectable when unmounted.",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist && flow-copy-source src dist",
    "lint": "eslint src/** __tests__/**",
    "test": "jest",
    "validate": "npm run lint && flow && npm test",
    "precommit": "lint-staged",
    "prepush": "npm run validate",
    "prepublish": "npm run validate && npm run build"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hjylewis/trashable-react.git"
  },
  "keywords": [
    "react",
    "promise",
    "javascript",
    "cancelable",
    "garbage-collect",
    "trashable"
  ],
  "author": "hjylewis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hjylewis/trashable-react/issues"
  },
  "homepage": "https://github.com/hjylewis/trashable-react#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "eslint": "^4.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.89.0",
    "flow-copy-source": "^1.2.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^5.0.0",
    "prettier": "1.8.2",
    "react": "^16.1.0",
    "react-dom": "^16.1.0"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "trashable": "^1.0.6"
  }
}
